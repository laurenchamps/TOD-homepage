APHY\n\n%heading {\n  font-family: $font-primary;\n  font-weight: 300;\n}\n\n.heading-1 {\n  @extend %heading;\n  font-size: 3.4rem;\n  color: #fff;\n}\n\n.heading-2 {\n  @extend %heading;\n  font-size: 2.4rem;\n  line-height: 1;\n}\n\n.heading-3 {\n  @extend %heading;\n  font-size: 1.8rem;\n}\n","scss/_projects.scss":"@use 'base';\n\n.projects {\n  font-family: base.$font-display;\n  padding: 12vh 8vw;\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(25rem, 1fr));\n  grid-template-rows: 5rem;\n  grid-auto-rows: minmax(30rem, max-content);\n  align-items: start;\n  column-gap: 3rem;\n  row-gap: 4rem;\n\n  & > .heading-2 {\n    grid-column: 1 / -1;\n    align-self: end;\n  }\n}\n\n.project {\n  display: grid;\n  grid-template-columns: max-content 1fr;\n  height: 100%;\n  align-content: start;\n  gap: 1rem;\n  box-shadow: 0 1.5rem 4rem rgba(#000, 0.15);\n\n  &__img {\n    grid-column: 1 / -1;\n    height: 15rem;\n\n    &--1 {\n      background-color: base.$colour-secondary-dark;\n    }\n\n    &--2 {\n      background-color: base.$colour-blue-light;\n    }\n\n    &--3 {\n      background-color: base.$colour-emerald;\n    }\n    &--4 {\n      background-color: base.$colour-green-light;\n    }\n    &--5 {\n      background-color: base.$colour-purple;\n    }\n    &--6 {\n      background-color: base.$colour-yellow;\n    }\n  }\n\n  &__description {\n    grid-column: 1 / -1;\n    font-size: 1.3rem;\n    padding: 0 1rem 2rem 1rem;\n  }\n\n  &__icons {\n    grid-column: 2 / -1;\n    justify-self: end;\n    display: flex;\n    gap: 1rem;\n    align-items: center;\n    font-size: 1.4rem;\n  }\n\n  &__name,\n  &__icons {\n    padding: 0 1rem;\n  }\n}\n","scss/_about.scss":"@use 'base';\n\n.header::before {\n  content: '';\n  position: absolute;\n  inset: 0;\n  clip-path: polygon(0 0, 100% 0, 100% 30%, 0 60%);\n  background-color: base.$colour-primary-dark;\n  z-index: -2;\n}\n\n.about {\n  font-family: base.$font-display;\n  min-height: 30rem;\n  display: grid;\n  grid-template-columns: 40% 60%;\n  position: relative;\n  margin-top: 6vh;\n\n  @media only screen and (max-width: base.$bp-large) {\n    display: block;\n    max-width: 80%;\n    margin: 8rem auto;\n  }\n\n  @media only screen and (max-width: base.$bp-medium) {\n    display: flex;\n    flex-direction: column;\n  }\n\n  &__img {\n    box-shadow: 0 1.5rem 4rem rgba(#000, 0.15);\n    margin-left: 8vw;\n    margin-top: 6vh;\n    grid-column: 1 / 2;\n    z-index: 10;\n    position: relative;\n    background: black;\n\n    @media only screen and (max-width: base.$bp-large) {\n      float: left;\n      margin: 0;\n      margin-right: 30px;\n      margin-bottom: 20px;\n      z-index: 10;\n    }\n\n    @media only screen and (max-width: base.$bp-medium) {\n      float: none;\n      margin: 0;\n    }\n\n    & img {\n      width: 100%;\n      display: block;\n      object-fit: cover;\n      object-position: right;\n\n      @media only screen and (max-width: base.$bp-large) {\n        max-width: 350px;\n      }\n\n      @media only screen and (max-width: base.$bp-medium) {\n        max-width: 100%;\n      }\n    }\n\n    & h1 {\n      position: absolute;\n      bottom: 0;\n      z-index: 10;\n      width: 100%;\n      text-align: center;\n      background-color: rgba(base.$colour-primary-dark, 0.6);\n    }\n  }\n\n  &__content {\n    position: relative;\n    padding: 4vh 4vw;\n    grid-column: 2 / -1;\n    margin: 10rem 8vw 6rem 0;\n    z-index: -1;\n\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    gap: 2rem;\n\n    &::before {\n      content: '';\n      position: absolute;\n      inset: 0;\n      background-color: #fff;\n      z-index: -1;\n      box-shadow: 0 1.5rem 4rem rgba(#000, 0.15);\n\n      @media only screen and (max-width: base.$bp-large) {\n        margin: 0 8rem 8rem 0;\n        transform: translate(10rem, 10rem);\n      }\n\n      @media only screen and (max-width: base.$bp-medium) {\n        transform: none;\n        margin: 0;\n      }\n    }\n\n    @media only screen and (max-width: base.$bp-large) {\n      display: block;\n      padding: 15rem 2rem 1rem 15rem;\n      margin: 0 0 0 1rem;\n    }\n\n    @media only screen and (max-width: base.$bp-medium) {\n      padding: 2rem;\n      margin: 0;\n    }\n\n    & .heading-2 {\n      @media only screen and (max-width: base.$bp-large) {\n        margin-bottom: 3rem;\n      }\n    }\n  }\n\n  &__icons {\n    font-size: 1.6rem;\n    display: flex;\n    gap: 1rem;\n    justify-content: flex-end;\n\n    @media only screen and (max-width: base.$bp-large) {\n      margin-top: 3rem;\n    }\n  }\n}\n","scss/_contact.scss":"@use 'base';\n\n.contact {\n  background-color: base.$colour-primary-dark;\n  color: #fff;\n  font-size: 1.6rem;\n  font-family: base.$font-display;\n  padding: 12vh 8vw;\n\n  display: grid;\n  grid-template-columns: minmax(min-content, 1fr) 1fr;\n  gap: 8rem;\n\n  @media only screen and (max-width: base.$bp-medium) {\n    display: flex;\n    flex-direction: column;\n  }\n\n  &__content {\n    grid-column: 1 / 2;\n    display: flex;\n    flex-direction: column;\n    gap: 1.4rem;\n\n    & .heading-2 {\n      margin-bottom: 1.6rem;\n    }\n  }\n\n  &__email,\n  &__phone {\n    display: flex;\n    gap: 1.2rem;\n    align-items: center;\n\n    &--icon {\n      font-size: 2rem;\n    }\n  }\n\n  &__icon-group {\n    font-size: 2.2rem;\n    display: flex;\n    gap: 1.4rem;\n    color: #000;\n    margin-top: 2rem;\n  }\n\n  &__img {\n    grid-column: 2 / -1;\n    object-fit: cover;\n    width: 100%;\n    display: block;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"ef510b9eeb67ab0ae3439d7707d18e84","cacheData":{"env":{}}}