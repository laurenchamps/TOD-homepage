amily: base.$font-display;\n  padding: 12vh 8vw;\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(25rem, 1fr));\n  grid-template-rows: 5rem;\n  grid-auto-rows: minmax(30rem, max-content);\n  align-items: start;\n  column-gap: 3rem;\n  row-gap: 4rem;\n\n  & > .heading-2 {\n    grid-column: 1 / -1;\n    align-self: end;\n  }\n}\n\n.project {\n  display: grid;\n  grid-template-columns: max-content 1fr;\n  height: 100%;\n  align-content: start;\n  gap: 1rem;\n  box-shadow: 0 1.5rem 4rem rgba(#000, 0.15);\n\n  &__img {\n    grid-column: 1 / -1;\n    height: 15rem;\n\n    &--1 {\n      background-color: base.$colour-secondary-dark;\n    }\n\n    &--2 {\n      background-color: base.$colour-blue-light;\n    }\n\n    &--3 {\n      background-color: base.$colour-emerald;\n    }\n    &--4 {\n      background-color: base.$colour-green-light;\n    }\n    &--5 {\n      background-color: base.$colour-purple;\n    }\n    &--6 {\n      background-color: base.$colour-yellow;\n    }\n  }\n\n  &__description {\n    grid-column: 1 / -1;\n    font-size: 1.3rem;\n    padding: 0 1rem 2rem 1rem;\n  }\n\n  &__icons {\n    grid-column: 2 / -1;\n    justify-self: end;\n    display: flex;\n    gap: 1rem;\n    align-items: center;\n    font-size: 1.4rem;\n  }\n\n  &__name,\n  &__icons {\n    padding: 0 1rem;\n  }\n}\n","scss/_about.scss":"@use 'base';\n\n.about {\n  font-family: base.$font-display;\n  min-height: 30rem;\n  display: grid;\n  grid-template-columns: 40% 60%;\n  position: relative;\n\n  &:before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    clip-path: polygon(0 0, 100% 0, 100% 55%, 0 100%);\n    background-color: base.$colour-primary-dark;\n  }\n\n  &__img {\n    box-shadow: 0 1.5rem 4rem rgba(#000, 0.15);\n    margin-left: 8vw;\n    margin-top: 6vh;\n    grid-column: 1 / 2;\n    z-index: 1;\n    position: relative;\n    background: black;\n\n    & img {\n      width: 100%;\n      display: block;\n      object-fit: cover;\n      opacity: 0.8;\n    }\n\n    & h1 {\n      position: absolute;\n      bottom: 0;\n      z-index: 10;\n      width: 100%;\n      text-align: center;\n    }\n  }\n\n  &__content {\n    padding: 4vh 4vw;\n    background-color: #fff;\n    grid-column: 2 / -1;\n    margin: 6rem 8vw 6rem 0;\n    max-height: 60%;\n    z-index: 1;\n    box-shadow: 0 1.5rem 4rem rgba(#000, 0.15);\n\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    gap: 2rem;\n  }\n\n  &__icons {\n    font-size: 1.6rem;\n    display: flex;\n    gap: 1rem;\n    justify-content: flex-end;\n  }\n}\n","scss/_contact.scss":"@use 'base';\n\n.contact {\n  background-color: base.$colour-primary-dark;\n  color: #fff;\n  font-size: 1.6rem;\n  font-family: base.$font-display;\n  padding: 12vh 8vw;\n\n  display: grid;\n  grid-template-columns: repeat(2, 1fr);\n  gap: 8rem;\n\n  &__content {\n    grid-column: 1 / 2;\n\n    display: flex;\n    flex-direction: column;\n    gap: 1.4rem;\n\n    & .heading-2 {\n      margin-bottom: 1.6rem;\n    }\n  }\n\n  &__email,\n  &__phone {\n    display: flex;\n    gap: 1.2rem;\n    align-items: center;\n\n    &--icon {\n      font-size: 2rem;\n    }\n  }\n\n  &__icon-group {\n    font-size: 2.2rem;\n    display: flex;\n    gap: 1.4rem;\n    color: #000;\n    margin-top: 2rem;\n  }\n\n  &__img {\n    grid-column: 2 / -1;\n    object-fit: cover;\n    width: 100%;\n    display: block;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"725dffdbdf2591869bcdd4f1edca17c8","cacheData":{"env":{}}}